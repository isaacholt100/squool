{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es5",
      "ScriptHost",
      "WebWorker",
      "WebWorker.ImportScripts"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "noUnusedLocals": true,
    //"noImplicitAny": true
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules",
    "src/components/book"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/pages/socketio.jsx", "public/json/script.js"  ]
}
